{
  "expo": {
    "name": "demo-react-native",
    "slug": "demo-react-native",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#050624"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "usesAppleSignIn": true,
      "bundleIdentifier": "com.scyuenab.demoreactnative",
      "infoPlist": {
        "CFBundleAllowMixedLocalizations": true
      }
    },
    "locales": {
      "ja": "./languages/ja.json",
      "en": "./languages/en.json",
      "zh": "./languages/zh-HK.json"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["expo-apple-authentication", "expo-localization"],
    "extra": {
      "eas": {
        "projectId": "096b25fb-91e1-4331-96bd-65cbd5373e03"
      }
    },
    "experiments": {
      "tsconfigPaths": true
    }
  }
}
